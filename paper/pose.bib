@book{claeskens_model_2008,
	address = {Cambridge; New York},
	title = {Model selection and model averaging},
	isbn = {9780521852258 0521852250},
	abstract = {Given a data set, you can fit thousands of models at the push of a button, but how do you choose the best? With so many candidate models, overfitting is a real danger. Is the monkey who typed Hamlet actually a good writer?" "Choosing a suitable model is central to all statistical work with data. Selecting the variables for use in a regression model is one important example. The past two decades have seen rapid advances both in our ability to fit models and in the theoretical understanding of model selection needed to harness this ability, yet this book is the first to provide a synthesis of research from this active field, and it contains much material previously difficult or impossible to find. In addition, it gives practical advice to the researcher confronted with conflicting results." "Model choice criteria are explained, discussed and compared, including Akaike's information criterion {AIC}, the Bayesian information criterion {BIC} and the focused information criterion {FIC}. Importantly, the uncertainties involved with model selection are addressed, with discussions of frequentist and Bayesian methods. Finally, model averaging schemes, which combine the strengths of several candidate models, are presented."--Jacket.},
	language = {English},
	publisher = {Cambridge University Press},
	author = {Claeskens, Gerda and Hjort, Nils Lid},
	year = {2008},
	file = {9780521852258_frontmatter.pdf:/Users/mtaddy/Library/Application Support/Firefox/Profiles/covgds8f.default/zotero/storage/M5EI5NXB/9780521852258_frontmatter.pdf:application/pdf}
